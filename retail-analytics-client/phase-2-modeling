        "df['date'] = pd.to_datetime(df['date'])\n",
        "\n",
        "monthly_sales = (\n",
        "    df.groupby(pd.Grouper(key='date', freq='M'))['revenue']\n",
        "      .sum()\n",
        "      .reset_index()\n",
        ")"
